{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
      },
      "dest": "/server/server.js"
    }
  ]
}
